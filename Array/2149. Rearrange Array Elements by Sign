

class Solution:
    def rearrangeArray(self, nums: List[int]) -> List[int]:
        p=[]
        n=[]
        a=[]
        for i in nums:
            if i>=0:
                p.append(i)
            else: 
                n.append(i)
        for j in range (len(p)):
            a.append(p[j])
            a.append(n[j])
        return a
