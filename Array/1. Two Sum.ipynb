      "source": [
        "class Solution(object):\n",
        "    def twoSum(self, nums, target):\n",
        "        n=len(nums)\n",
        "        for i in range (n-1):\n",
        "            for j in range(i+1,n):\n",
        "                if nums[i]+nums[j]==target:\n",
        "                    return (i,j)\n",
        "        return []"
      ]
    }
  ]
